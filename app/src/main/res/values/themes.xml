    <style name="Theme.AndroidLabProject.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AndroidLabProject.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
