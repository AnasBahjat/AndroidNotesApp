<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.AndroidLabProject" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="BUTTON">
        <item name="android:background">#2596be</item>
    </style>


    <style name="SCREEN">
        <item name="android:background">#F9F1F1</item>
    </style>


    <style name="ERRORSCREEN">
        <item name="android:background">#FFFFFE</item>
    </style>

    <style name="textColor">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="EdtTxt">
        <item name="android:background">#FFF</item>
